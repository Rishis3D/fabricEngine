        "extDeps" : {},
        "code" : "dfgEntry {\n\n  outPos.resize(4*num);\n  \n  Vec3 offset = Vec3();\n  for (Size i = 0; i< num; i++) {\n    \n    offset.x = (i%numPerRow)*sizeX+spaceX*(i%numPerRow);\n    //offset.y = i*sizeY+spaceX*i;\n    offset.y = i / numPerRow;\n    \n    outPos[i*4] = Vec3(0,0,0) + offset;\n    outPos[i*4+1] = Vec3(sizeX,0,0) + offset;\n    outPos[i*4+2] = Vec3(sizeX,sizeY,0) + offset;\n    outPos[i*4+3] = Vec3(0,sizeY,0) + offset;\n  }\n  \n}\n"
        }
