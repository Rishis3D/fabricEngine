        "extDeps" : {},
        "code" : "dfgEntry {\n  \n  Integer numPoints = inPos.size();\n  \n  PolygonMesh mesh();\n  mesh.createPoints(numPoints);\n  \n  mesh.beginStructureChanges();\n\n  Size polygonSizes[];\n  Size packedPolygonPointIndices[];\n\n  \n  Integer offset;\n  Vec3 ac();\n  Vec3 bd();\n  for (Size i = 0; i < num; i++){    \n      offset = i * 4;    \n      \n      polygonSizes.push(4);\n      packedPolygonPointIndices.push(offset);\n      packedPolygonPointIndices.push(offset+1);\n      packedPolygonPointIndices.push(offset+2);\n      packedPolygonPointIndices.push(offset+3);\n        \n      mesh.setPointPosition(offset, inPos[offset]);\n      mesh.setPointPosition(offset+1, inPos[offset+1]);\n      mesh.setPointPosition(offset+2, inPos[offset+2]);\n      mesh.setPointPosition(offset+3, inPos[offset+3]);      \n  }\n        \n  mesh.addPolygons(polygonSizes, packedPolygonPointIndices);\n  mesh.endStructureChanges();\n  mesh.recomputePointNormals();\n  \n  outMesh = mesh;\n}"
        }
      },
