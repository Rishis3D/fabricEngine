            }
          ],
        "extDeps" : {},
        "code" : "dfgEntry {\n  \n  Integer numPoints = inPos.size();\n  \n  // create mesh data\n  PolygonMesh mesh();\n  mesh.createPoints(numPoints);\n  mesh.beginStructureChanges();\n  Size polygonSizes[];\n  Size packedPolygonPointIndices[];\n  \n  // set the polygon connections\n  Integer a, b, c, d, row;\n  for (Size i = 0; i < numPoints-numRadial; i++){\n    \n    row = i / numRadial;\n    \n    // skip to join the last row with the next row\n    if (row % numAxial == numAxial-1) continue;\n    \n    a = i;\n    b = (i+1) % numRadial + numRadial * row;\n    c = b + numRadial;\n    d = a + numRadial;\n    //report(a + \" \" + b + \" \" + c + \" \" + d);\n  \n    polygonSizes.push(4);\n    packedPolygonPointIndices.push(a);\n    packedPolygonPointIndices.push(b);\n    packedPolygonPointIndices.push(c);\n    packedPolygonPointIndices.push(d);\n  }\n    \n  // set the pos of the vertices\n  for (Size i = 0; i < numPoints; i++){\n    mesh.setPointPosition(i, inPos[i]);\n  }\n  \n  mesh.addPolygons(polygonSizes, packedPolygonPointIndices);\n  mesh.endStructureChanges();\n  mesh.recomputePointNormals(smoothAngle);\n  \n  outMesh = mesh;\n}"
        }
