        "extDeps" : {},
        "code" : "dfgEntry {
  
  Integer num = targetPos.size;
  outPos.resize(num);
  xfos.resize(num);
  currPos.resize(num);
  
  // reset the sim on first frame
  if (time <= 1) {
    
    for (Size i = 0; i < num; i++) {
      Vec3 v = targetPos[i];
      outPos[i] = v;
      xfos[i] = Xfo(v);
    }  
  }
  
  // do simulation
  else {
  
    for (Size i = 0; i < num; i++) {
      Vec3 v = ((targetPos[i] - currPos[i]) * easeFactor) + currPos[i];
      outPos[i] = v;
      //report(v);
      xfos[i] = Xfo(v);
    }
  
  
  }
}
