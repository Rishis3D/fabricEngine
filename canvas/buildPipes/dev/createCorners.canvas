          ],
        "extDeps" : {
          "Math" : "*"
          },
        "code" : "require Math;\n\ndfgEntry {\n\n  \n  Size numPos = pos.size;\n\n  if (numPos < 2)\n    return;\n\n  // create a quaternion to use to construct the xfos\n  Quat q = Quat();\n  \n  Integer numCorners = pos.size-2;\n  transformedCenter.resize(numCorners);\n  resultRadPos.resize(numPoints * numCorners);\n  resultRadXfos.resize(numPoints * numCorners);\n  fittingXfos.resize(numCorners*2+2);\n  \n  //report(resultRadXfos.size);\n  //report(numCorners);\n\n  for (Integer i = 0; i < numCorners; i++){\n  \n    Vec3 vecAim = pos[i] - pos[i+1];\n    Vec3 vecUp = pos[i+2] - pos[i+1];\n    Vec3 p = pos[i+1];\n    vecAim.normalize();\n    vecUp.normalize();\n  \n    // construct a orthogonalized coordinate space\n    Vec3 vecCross = vecAim.cross(vecUp);\n    Vec3 vecUpOrtho = vecCross.cross(vecAim);\n    vecCross.normalize();\n    vecUpOrtho.normalize();\n  \n  \n    Float32 upU = vecUp.dot(vecAim);\n    Float32 upV = vecUp.dot(vecUpOrtho);\n    \n    Float32 theta = atan2(upV, upU) * 0.5;\n    Float32 angFull = ((3.1415/2) - theta) * 2;\n  \n    Float32 adjacent = radius / (tan(theta));\n    Vec3 radiusCenter = Vec3(0, radius, adjacent);\n  \n  \n    //Quat q = Quat();\n    Xfo xfo = Xfo();\n    xfo.ori = q.setFromDirectionAndUpvector(vecAim, vecUp);\n    xfo.tr = p;\n  \n    // plot the pos\n    // calculate the positions on the circle\n    Float32 angInc = angFull/(numPoints-1);\n  \n    //the ang from pos x axis to opposite (negative y axis)\n    Float32 angToOpp = 1.5 * 3.1415; // math.pi\n    \n    Vec3 lastVec;\n    Xfo lastRadXfo;\n    Xfo firstRadXfo;\n    \n    for (Integer j = 0; j < numPoints; j++){\n\n      Float32 a = angToOpp - (angInc * j);\n      Float32 x = cos(a) * radius + adjacent;\n      Float32 y = sin(a) * radius + radius;\n      Vec3 v = xfo.transformVector(Vec3(0,y,x));\n      resultRadPos.push(v);\n      \n      // get a vector form last pos on crv to this...\n      if (j > 0){\n        \n        if (j < numPoints-1){\n        \n          Vec3 nv = v - lastVec;\n          Xfo cornXfoMid = Xfo();\n          cornXfoMid.ori = q.setFromDirectionAndUpvector(nv, vecCross);\n          cornXfoMid.tr = v;\n          resultRadXfos.push(cornXfoMid);\n        }\n        else {\n          Xfo cornXfo = Xfo();\n          cornXfo.tr = v;\n          cornXfo.ori = q.setFromDirectionAndUpvector(vecUp, vecCross);\n          resultRadXfos.push(cornXfo);\n          lastRadXfo = cornXfo;\n        }\n      }\n      \n      else {\n        Xfo cornXfo = Xfo();\n        cornXfo.tr = v;\n        cornXfo.ori = q.setFromDirectionAndUpvector(-vecAim, vecCross);\n        resultRadXfos.push(cornXfo);\n        firstRadXfo = cornXfo;\n      }\n      \n      lastVec = v;\n    }\n    \n    \n    // add the fitting transforms\n    \n    if (i == 0){\n      // add the fitting xfos\n      Xfo aXfo = Xfo();\n      aXfo.tr = pos[0];\n      aXfo.ori = q.setFromDirectionAndUpvector(-vecAim, vecCross);\n      fittingXfos.push(aXfo);\n    }\n    \n    else if (i == numCorners-1) {\n      Xfo bXfo = Xfo();\n      bXfo.tr = pos[i+2];\n      bXfo.ori = q.setFromDirectionAndUpvector(vecUp, vecCross);\n      fittingXfos.push(bXfo);\n    }\n    \n    // fitting for the corners\n    fittingXfos.push(lastRadXfo.clone());\n    fittingXfos.push(firstRadXfo.clone());\n    \n    // the radius center position\n    transformedCenter.push(xfo.transformVector(radiusCenter));\n    \n    report(resultRadXfos.size);\n  }\n}\n"
        },
      "title" : "pipeTool"
      },
