        "extDeps" : {
          "petTool" : "*"
          },
        "code" : "require Geometry;

dfgEntry {
    
  Integer numVecPos = inVec.size;
  
  Vec3 tempCenter[];
  Xfo tempRadiusXfos[];
  Xfo parentXfos[];
  PolygonMesh meshes[];
  Xfo tempPipeXfos[];
  Float32 pipeLength[];
  Float32 adjacentLength[];
  Xfo tempPipeDebugXfos[];
  
  for (Size i = 0; i < numVecPos; i++){
  
    // first pos
    if (i == 0){
      
      // calculate the aim, up vectors
      Vec3 aimVec = inVec[i+1] - inVec[i]; 
      Vec3 upVec = inVec[i+2] - inVec[i];
      pipeLength.push(aimVec.length);
      
      aimVec.normalize();
      upVec.normalize();
      Vec3 crossVec = aimVec.cross(upVec);
      upVec = aimVec.cross(crossVec);
      
      Xfo parentXfo = Xfo();
      parentXfo.sc = Vec3(1,1,1);
      parentXfo.ori = parentXfo.ori.setFromDirectionAndUpvector(aimVec, upVec);
      parentXfo.tr = inVec[i];
      tempPipeXfos.push(parentXfo);
      
      
    }
    // last pos
    else if (i == numVecPos-1){}
    // inbetween pos
    else {
      
      // calculate the aim, up vectors
      Vec3 aimVec = inVec[i+1] - inVec[i];
      Vec3 upVec = inVec[i-1] - inVec[i]; 
      pipeLength.push(aimVec.length);
      
      aimVec.normalize();
      upVec.normalize();
      
      Xfo parentXfo = Xfo();
      parentXfo.sc = Vec3(1,1,1);
      parentXfo.ori = parentXfo.ori.setFromDirectionAndUpvector(aimVec, upVec);
      parentXfo.tr = inVec[i];
      parentXfos.push(parentXfo);
      tempPipeXfos.push(parentXfo);
                  
      Float32 angle = acos(aimVec.dot(upVec));
            
      
      // calculate the radius pos
      
      Float32 angFull = 3.1415 - angle;
  
      //offset with half pi
      Float32 angOffset = -1.5708;

      
      Float32 adjacent = radius / (tan(angle*.5));
      adjacentLength.push(adjacent);
            
      Float32 angInc = angFull/(numRadiusPos-1);
  
      // calculate the corner pos      
      // xfo to extrude corner
      Xfo radExtXfos[];
      for (Size j = 0; j < numRadiusPos; j++) {
  
        Float32 a =  angOffset - angInc * j;
        Float32 x = cos(a) * radius + adjacent;
        Float32 y = sin(a) * radius + radius;
        
        Xfo xfo = Xfo();
        xfo.tr = Vec3(0, y, x);
        xfo.ori = xfo.ori.setFromEuler(Vec3((angInc * j),0,0));
        tempRadiusXfos.push(parentXfo * xfo);
        
        radExtXfos.push(xfo);
                
      }
  
      tempCenter.push(parentXfo.transformVector(Vec3(0, radius, adjacent)));
      
      // create the mesh
      PolygonMesh mesh = PolygonMesh();
      //mesh.addExtrusion(radExtXfos, profile, true);
      mesh.extrude(radExtXfos, profile, true, true, maxU, maxV);
      
      meshes.push(mesh);
    
    }
  }
  
  
  // create the pipe transforms
  PolygonMesh pipeMeshes[];
  for (Integer k = 0; k < numVecPos-1; k++){
  
    Float32 uInc;
    Float32 length;
    
    if (k == 0){
      length = (pipeLength[0] - adjacentLength[0]);
      uInc =  length / (numPipeDivision-1);
    }
    else if (k == numVecPos-2){
      length = (pipeLength[k] - adjacentLength[k-1]);
      uInc =  length / (numPipeDivision-1);
      length += adjacentLength[k-1];
    }
    else {
      length = pipeLength[k] - (adjacentLength[k-1] + adjacentLength[k]);
      uInc =  length / (numPipeDivision-1);
      length += adjacentLength[k-1];
    }
    
    Xfo pipeExtXfos[];
    
    for (Size m = 0; m < numPipeDivision; m++){
      Xfo x = Xfo();
      x.sc = Vec3(pipeScale,pipeScale,pipeScale);
      x.tr.z = length + (-uInc * m);
      tempPipeDebugXfos.push(tempPipeXfos[k] * x);
      pipeExtXfos.push(x);
    }
    
    // create the mesh
    PolygonMesh mesh = PolygonMesh();
    //mesh.addExtrusion(pipeExtXfos, profile, true);
    mesh.extrude(pipeExtXfos, profile, true, true, maxU, maxV);
    pipeMeshes.push(mesh);
    
  }
  
  center.resize(numVecPos-2);
  radiusXfos.resize((numVecPos-2) * numRadiusPos);
  pipeXfos.resize(numVecPos-1);
  pipeDebugXfos.resize((numVecPos-1)*5);
  
  center = tempCenter;
  radiusXfos = tempRadiusXfos;
  pipeXfos = tempPipeXfos;
  pipeDebugXfos = tempPipeDebugXfos;
  


  // merge the radius meshes  
  PolygonMesh emptyMesh = PolygonMesh();  
  emptyMesh.mergeMeshes(meshes, parentXfos, false);  
  emptyMesh.recomputePointNormals(0.5);
  
  radiusMesh = emptyMesh;
  
  
  // merge the pipe meshes 
 
  PolygonMesh emptyPipeMesh = PolygonMesh();  
  emptyPipeMesh.mergeMeshes(pipeMeshes, tempPipeXfos, false);  
  emptyPipeMesh.recomputePointNormals(0.5);
  
  pipeMesh = emptyPipeMesh;

}
"
