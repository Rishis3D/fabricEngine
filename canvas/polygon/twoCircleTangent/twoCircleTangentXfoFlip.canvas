        "code" : "dfgEntry {
  
  Vec3 temp[];
  temp.push(Vec3(0,0,0));
  temp.push(Vec3(0,0,3));
  temp.push(Vec3(0,3,3));
  temp.push(Vec3(0,6,0));

  

  Float32 maxY = 0;
  for (Size i = 0; i <temp.size(); i++){
    if (temp[i].y > maxY){
      maxY = temp[i].y;
    }
  }
  for (Size i = 0; i <temp.size(); i++){
    temp[i] = (temp[i] / maxY) * profileScale;
  }
  
  profile.resize(temp.size());
  profile = temp;
}
