          {
            "objectType" : "Port",
            "nodePortType" : "In",
            "name" : "outCornerPos",
            "execPortType" : "Out",
            "typeSpec" : "Vec3[]"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "extrudeSize",
            "execPortType" : "In",
            "typeSpec" : "Float32"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "extrudeOffsetZ",
            "execPortType" : "In",
            "typeSpec" : "Float32"
            },
          {
            "objectType" : "Port",
            "nodePortType" : "Out",
            "name" : "backOffsetZ",
            "execPortType" : "In",
            "typeSpec" : "Float32"
            }
          ],
        "extDeps" : {},
        "code" : "dfgEntry {\n  \n  // the number of windows\n  Integer num = inPos.size() / 4;\n  \n  Integer numInPoints = inPos.size();\n  outCornerPos.resize(numInPoints*3);\n        \n  Integer inOffset, outOffset;\n  Vec3 a(extrudeSize,extrudeSize,-extrudeOffsetZ);\n  Vec3 b(-extrudeSize,extrudeSize,-extrudeOffsetZ);\n  Vec3 c(-extrudeSize,-extrudeSize,-extrudeOffsetZ);\n  Vec3 d(extrudeSize,-extrudeSize,-extrudeOffsetZ);\n  \n  Vec3 a1 = Vec3(a.x, a.y, a.z - backOffsetZ);\n  Vec3 b1 = Vec3(b.x, b.y, b.z - backOffsetZ);\n  Vec3 c1 = Vec3(c.x, c.y, c.z - backOffsetZ);\n  Vec3 d1 = Vec3(d.x, d.y, d.z - backOffsetZ);\n  \n  \n  for (Size i = 0; i < num; i++){    \n      outOffset = i * (4 * 3); \n      inOffset = i * 4; \n      \n      outCornerPos[outOffset] = inPos[inOffset];\n      outCornerPos[outOffset+1] = inPos[inOffset+1];\n      outCornerPos[outOffset+2] = inPos[inOffset+2];\n      outCornerPos[outOffset+3] = inPos[inOffset+3];\n         \n      outCornerPos[outOffset+4] = inPos[inOffset] + a;\n      outCornerPos[outOffset+5] = inPos[inOffset+1] + b;\n      outCornerPos[outOffset+6] = inPos[inOffset+2] + c;\n      outCornerPos[outOffset+7] = inPos[inOffset+3] + d;\n      \n      outCornerPos[outOffset+8] = inPos[inOffset] + a1;\n      outCornerPos[outOffset+9] = inPos[inOffset+1] + b1;\n      outCornerPos[outOffset+10] = inPos[inOffset+2] + c1;\n      outCornerPos[outOffset+11] = inPos[inOffset+3] + d1;\n  }\n  \n}"
        },
