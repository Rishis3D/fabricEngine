        "code" : "dfgEntry {
  
  Integer numX = inPosX.size;
  Integer numY = inPosY.size;
  
  if (numX < 3 || numY < 3) {
    return;
  }
        
  Integer numPoints = numY * numX;
  PolygonMesh mesh();
  mesh.createPoints(numPoints);
  
  mesh.beginStructureChanges();

  Size polygonSizes[];
  Size packedPolygonPointIndices[];
  
  Integer a, b, c, d;
  Vec3 v();
  
  for (Size i = 0; i < numY; i++){
    
    v.y = inPosY[i].y;
    
    for (Size j = 0; j < numX; j++){
      
      v.x = inPosX[j].x;
      mesh.setPointPosition(i*numX+j, v);
      
      // skip the windows
      if (j % 2 == 1 && i % 2 == 1){
        continue;        
      } 
      
      if (i < numY-1 && j < numX-1){
        a = j+i*numX;
        b = a+1;
        d = (i+1)*numX+j;
        c = d+1;
        //report(a+\",\"+b+\",\"+c+\",\"+d);
        polygonSizes.push(4);
        packedPolygonPointIndices.push(a);
        packedPolygonPointIndices.push(b);
        packedPolygonPointIndices.push(c);
        packedPolygonPointIndices.push(d);
      }
    }
  }
      
  mesh.addPolygons(polygonSizes, packedPolygonPointIndices);
  mesh.endStructureChanges();
  mesh.recomputePointNormals();
  
  // create the uvs
  GeometryAttributes attributes = mesh.getAttributes();
  Ref<Vec2Attribute> uvs = attributes.getOrCreateAttribute(\"uvs0\", Vec2Attribute);
  
  Float32 maxU = inPosX[numX-1].x;
  Float32 maxV = inPosY[numY-1].y;
  
  Integer numWinX = (numX-1) / 2;
  Integer numWinY = (numY-1) / 2;
  Integer numWinXPolys = (numX-1) - numWinX;
  
  Integer winRow, WinPolyOffset, fullRow, polygonIndex;
  for (Size i = 0; i < numY-1; i++){
  
    winRow = i / 2;
    WinPolyOffset = winRow * numWinXPolys;
    
    for (Size j = 0; j < numX-1; j++){
      
      // even rows
      if (i % 2 == 0) {
      
        polygonIndex = winRow * (numX-1)+ WinPolyOffset + j;

        // not the last column
        if (j < numX-2){
          mesh.setPolygonAttribute(polygonIndex, Size(0), uvs, Vec2(inPosX[j].x/maxU,inPosY[i].y/maxV));
          mesh.setPolygonAttribute(polygonIndex, Size(3), uvs, Vec2(inPosX[j].x/maxU,inPosY[i+1].y/maxV));
          //report(\"even... i: \" + i + \"j: \" + j);                   
          }
        
        // the last even column  
        else {
          mesh.setPolygonAttribute(polygonIndex, Size(0), uvs, Vec2(inPosX[j].x/maxU,inPosY[i].y/maxV));
          mesh.setPolygonAttribute(polygonIndex, Size(3), uvs, Vec2(inPosX[j].x/maxU,inPosY[i+1].y/maxV));
          mesh.setPolygonAttribute(polygonIndex, Size(1), uvs, Vec2(inPosX[j+1].x/maxU,inPosY[i].y/maxV));
          mesh.setPolygonAttribute(polygonIndex, Size(2), uvs, Vec2(inPosX[j+1].x/maxU,inPosY[i+1].y/maxV));
          //report(\"even last! i: \" + i + \"j: \" + j);
        }
      }
        
      // un-even rows
      else{
        
        // not last un-even row
        if (i < numY-2){
          //report(\"UN-even... i: \" + i + \"j: \" + j);
        }
        // last un-even row
        else{
          // on and even tile, i.e. not a window          
          if (j % 2 == 0){
            polygonIndex = ((i+1) /2) * (numX-1) + WinPolyOffset + (j / 2);
            //report(polygonIndex);
            mesh.setPolygonAttribute(polygonIndex, Size(2), uvs, Vec2(inPosX[j+1].x/maxU,inPosY[i+1].y/maxV));
            mesh.setPolygonAttribute(polygonIndex, Size(3), uvs, Vec2(inPosX[j].x/maxU,inPosY[i+1].y/maxV));
          }
        }
      }
    } 
  }
  
  outMesh = mesh;
}"
