          ],
        "extDeps" : {},
        "code" : "dfgEntry {
       
  // create the uvs
  GeometryAttributes attributes = inMesh.getAttributes();
  Ref<Vec2Attribute> uvs = attributes.getOrCreateAttribute(\"uvs0\", Vec2Attribute);
  Ref<Vec3Attribute> pos = attributes.getOrCreatePositions();
     
  //Size numVerts = uvs.size();
  Size numVerts = attributes.size();
  //report(attributes.size());
  Vec3 vertPos[] = pos.values;
  
  Size index;
  
  for (Size i = 0; i < numVerts / 2; i++){
    //index = i*4;
    //report(vertPos[i]);
    inMesh.setPolygonAttribute(0, Size(0), uvs, Vec2(0,0));
    inMesh.setPolygonAttribute(0, Size(1), uvs, Vec2(1,0));
    inMesh.setPolygonAttribute(0, Size(2), uvs, Vec2(1,1));
    inMesh.setPolygonAttribute(0, Size(3), uvs, Vec2(0,1));
  }
}"
        }
      }
